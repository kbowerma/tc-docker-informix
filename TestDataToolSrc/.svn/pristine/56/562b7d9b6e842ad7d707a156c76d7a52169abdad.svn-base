package com.cronos.onlinereview.test.data.commonoltp;

/**
 * <p>An enumeration over the terms of use. Corresponds to <code>common_oltp.terms_of_use</code> database table.</p>
 * 
 * @author isv
 * @version 1.0
 */
public enum TermsOfUse {
    
    REVIEWER_1(17, "Reviewer terms of use text"),
    
    REVIEWER_TERMS(20493, "Another terms of use text"),

    SUBMITTER_1(20543, "Submitter terms of use text 20543"),

    SUBMITTER_2(20623, "Submitter terms of use text"),
    
    SUBMITTER_3(20703, "Submitter terms of use text 20703"),
    
    REVIEWER_2(20704, "Reviewer terms of use text 20704"),
    
    CCA(20713, "CCA terms of use text 20713"),
    
    ASSIGNMENT_2_0(20753, "Assignment 2.0"),
    
    APPROVED_OR_MANAGERS(20794, "Approved OR Managers - TopCoder Technical Team"),
    
    MSA(20893, "MSA"),
    
    GOVERNMENT_ID(20963, "Government id term");

    /**
     * <p>A <code>long</code> providing the ID of this terms of use.</p>
     */
    private long termsOfUseId;

    /**
     * <p>A <code>String</code> providing the name of this terms of use.</p>
     */
    private String name;

    /**
     * <p>Constructs new <code>TermsOfUse</code> instance.</p>
     * 
     * @param termsOfUseId a <code>long</code> providing the ID of this terms of use.
     * @param name a <code>String</code> providing the name of this terms of use.
     */
    private TermsOfUse(long termsOfUseId, String name) {
        this.termsOfUseId = termsOfUseId;
        this.name = name;
    }

    /**
     * <p>Gets the name of this terms of use.</p>
     *
     * @return a <code>String</code> providing the name of this terms of use.
     */
    public String getName() {
        return this.name;
    }

    /**
     * <p>Gets the ID of this terms of use.</p>
     *
     * @return a <code>long</code> providing the ID of this terms of use.
     */
    public long getTermsOfUseId() {
        return this.termsOfUseId;
    }

}
